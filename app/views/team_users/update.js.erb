<% if team_user.errors.empty? %>
  $("#team_user_<%= team_user.id %> td div.role").html("<%= team_user.role %>");
  $("#change_role_team_user_<%= team_user.id %>").fadeToggle();
<% else %>
  $('#alert p').html("<%= escape_javascript(team_user.errors.full_messages.join('<br/>')) %>");
  $('#alert').fadeToggle();
<% end %>
